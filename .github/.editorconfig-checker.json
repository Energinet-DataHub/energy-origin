{
    "Verbose": false,
    "Debug": false,
    "IgnoreDefaults": true,
    "SpacesAfterTabs": false,
    "NoColor": false,
    "Exclude": [
        ".*\\.csproj$",
        "\\.sln$",
        "\\.props$",
        "\\.targets$",
        "\\.verified\\.",
        "\\.md$",
        "\\.sln$",
        "\\.xml$",
        "\\.js$",
        "\\.css$",
        "\\.cshtml$",
        "LICENSE",
        "bin/",
        "obj/",
        "packages/",
        "\\.nuget/",
        "node_modules/",
        "\\.git/",
        "\\.vs/",
        ".idea/**",
        "\\.html$",
        "domains/*/wwwroot/lib/**"
    ],
    "AllowedContentTypes": [
        "text/plain",
        "text/shellscript",
        "application/x-shellscript"
    ],
    "PassedFiles": [],
    "Disable": {
        "EndOfLine": false,
        "Indentation": false,
        "IndentSize": false,
        "InsertFinalNewline": false,
        "TrimTrailingWhitespace": false,
        "MaxLineLength": true
    }
}
