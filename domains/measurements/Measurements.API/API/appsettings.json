{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning",
            "System.Net.Http.HttpClient": "Warning"
        }
    },
    "AllowedHosts": "*",
    "Kestrel": {
        "Endpoints": {
            "Rest": {
                "Url": "http://*:80",
                "Protocols": "Http1"
            },
            "Grpc": {
                "Url": "http://*:81",
                "Protocols": "Http2"
            }
        }
    }
}
