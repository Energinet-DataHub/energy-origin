
{	"name": "Debian",
	"build": {
		"dockerfile": "Dockerfile",
		"args": { "VARIANT": "bullseye" }
	},
	"remoteUser": "vscode",
	"features": {
		"docker-from-docker": "20.10"
	},
	"mounts": [
		"source=${env:HOME}${env:USERPROFILE}/.kube,target=/usr/local/share/kube-localhost,type=bind"
	],
	"remoteEnv": {
		"SYNC_LOCALHOST_KUBECONFIG": "true"
	}
}
